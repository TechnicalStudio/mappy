{
    "name": "mappy/mappy",
    "description": "Real Time Maps",
    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "mappy"
    ],
    "support": {
        "docs": "https://github.com/TechnicalStudio/mappy",
        "issues": "https://github.com/TechnicalStudio/mappy"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tech Studio",
            "homepage": "https://technicalstudio.co.uk"
        }
    ],
    "require": {
        "craftcms/cms": "~3.0.0-beta.23"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
          "mappy\\mappy\\": "src/"
        }
    },
    "extra": {
        "name": "Mappy",
        "handle": "mappy",
        "schemaVersion": "1.0.0",
        "hasCpSettings": true,
        "hasCpSection": true,
        "components": {
            "mappyService": "mappy\\mappy\\services\\MappyService"
        },
        "class": "mappy\\mappy\\Mappy"
    }
}
